package stack;

public class stack_methods implements interface_methods {
    int p=0;
	@Override
	public void push(int number ) {
		// TODO Auto-generated method stub
		int[] stack_array = new int[6];
		stack_array[p]= number;
		
		p++;
		
		
		
	}

	@Override
	public void pop() {
		// TODO Auto-generated method stub
		p--;
		System.out.println("elements are as");
		for(int k=0;k<p;k++)
		System.out.println(k);	
	}

	@Override
	public int get_top() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public boolean isempty() {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public void display() {
		// TODO Auto-generated method stub
		System.out.println("elements are as");
		
	}

	
	
	

	
		
	}


